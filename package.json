{
  "name": "renpy-python-docsup",
  "license": "MIT",
  "displayName": "%extension.name%",
  "description": "Show docstring hover for Python code in .rpy files",
  "version": "0.0.1",
  "publisher": "LKDG",
  "engines": {
    "vscode": "^1.106.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EHLK/ren-py-python-docsup.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": ["renpy"],
  "activationEvents": ["Programming Languages"],
  "main": "./out/extension.js",
  "contributes": {
    "commands":[{
      "command": "renpy-symbol-index.rebuild",
      "title": "%command.rebuild.title%",
      "category":"renpy"
    }],
    "languages":[
      {
        "id": "renpy",
        "aliases": [
          "Ren'py",
          "renpy"
        ],
        "extensions": [
          ".rpy"
        ],
        "configuration": "./language-configuration.json"
      }
      
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
